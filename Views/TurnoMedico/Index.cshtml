@model IEnumerable<ORTSocial.Models.TurnoMedico>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Medico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Socio)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Medico.IdMedico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Socio.IdSocio)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.idTurno">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.idTurno">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.idTurno">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
